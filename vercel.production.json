{
  "version": 2,
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(css|js|images|fonts)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "VERCEL": "1",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "error",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views"
  },
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
